{
  "name": "alexa-nationalanthems-skill",
  "version": "0.0.0",
  "description": "An Alexa skill for the Amazon Echo.",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js test/**/*.js",
    "update-schema": "bin/update-schema --file model/schema.json",
    "prebuild": "npm run update-schema",
    "build": "npm-run-all --parallel build:skill",
    "build:skill": "webpack --config config/webpack.config.babel.js",
    "pretest": "npm run build",
    "test": "ava",
    "posttest": "npm run lint",
    "deploy:prod": "serverless deploy --stage production",
    "deploy:dev": "serverless deploy --stage dev"
  },
  "ava": {
    "babel": "inherit",
    "failWithoutAssertions": false
  },
  "devDependencies": {
    "alexa-annotations": "^9.2.0",
    "alexa-request": "^0.3.4",
    "alexa-response": "^4.2.10",
    "alexa-ssml-jsx": "^2.0.3",
    "alexa-utterances": "^0.2.1",
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "bestzip": "^1.1.4",
    "chai": "^4.1.2",
    "chai-subset": "^1.5.0",
    "cheerio": "^1.0.0-rc.2",
    "countryjs": "^1.8.0",
    "eslint": "^4.6.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.7",
    "node-aws-lambda": "^0.1.8",
    "nomnom": "^1.8.1",
    "npm-run-all": "^4.1.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "serverless": "^1.21.0",
    "webpack": "^3.5.5"
  },
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "alexa",
    "alexa-skill",
    "echo",
    "skill"
  ]
}
